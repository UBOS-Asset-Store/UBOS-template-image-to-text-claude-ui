                                                                "srcDoc": "import React from 'https://cdn.skypack.dev/react';\n\n    import ReactDOM from 'https://cdn.skypack.dev/react-dom';\n    import { Button, Container } from 'https://cdn.skypack.dev/@material-ui/core';\n    const MyCustomComponent = ({data, updateData, trigerParent}) => {\n   const [file, setFile] = React.useState();\n\t const [isLoading, setIsLoading] = React.useState(false);\n  const uploadRef = React.useRef();\n\n  const progressRef = React.useRef();\n\n  const UploadFile = () => {\n    setIsLoading(true);\n\t\tconst file = uploadRef.current.files[0];\n    setFile(URL.createObjectURL(file));\n    var formData = new FormData();\n    formData.append(\"file\", file);\n   formData.append(\"model\", data.model);\n\t formData.append(\"key\", data.key);\n\n    var xhr = new XMLHttpRequest();\n    xhr.upload.addEventListener(\"progress\", ProgressHandler, false);\n    xhr.addEventListener(\"load\", SuccessHandler, false);\n    xhr.addEventListener(\"error\", ErrorHandler, false);\n    xhr.addEventListener(\"abort\", AbortHandler, false);\n    xhr.open(\n      \"POST\",\n     data.url+\"/describeImage\"\n    );\n    xhr.send(formData);\n  };\n\n  const ProgressHandler = (e) => {\n    var percent = (e.loaded / e.total) * 100;\n    progressRef.current.value = Math.round(percent);\n  };\n\n  const SuccessHandler = (e) => {\n\t setIsLoading(false);\n    progressRef.current.value = 0;\n    uploadRef.current.value = '';\n    if(e.currentTarget && e.currentTarget.response){\n     trigerParent(e.currentTarget.response);\n    }\n  };\n  const ErrorHandler = (e) => {\n\t setIsLoading(false);\nprogressRef.current.value = 0;\nuploadRef.current.value = '';\n    if(e.currentTarget && e.currentTarget.response){trigerParent(e.currentTarget.response);\n    }\n  };\n  const AbortHandler = (e) => {\n\t\n    progressRef.current.value = 0;\n    uploadRef.current.value = '';\n    if(e.currentTarget && e.currentTarget.response){\n     trigerParent(e.currentTarget.response);\n    }\n  };\n    const mystyle = {\n      display:\"none\"\n    };\n\t\n\tconst wrap={\n\tposition:\"relative\",\n\t\twidth: \"100%\",\n    height: \"100%\",\n    display: \"flex\",\n    flexDirection: \"revert\",\n    alignItems: \"center\",\n    justifyContent: \"space-around\"\n\t\t\n\t\t}\n\t\t\n\t\tconst labelInput = {\nborder: \"1.5px solid #D97757\",\ntransition: \"all 0.3s ease 0s\",\npadding: \"2px 16px 4px 16px\",\nborderRadius: \"6px\",\nwidth: \"37%\",\nheight: \"25px\",\ntextAlign: \"center\",\nmargin: \"7px\",\ndisplay: \"flex\",\nalignItems: \"center\",\njustifyContent: \"center\",\ncolor: \"#D97757\",\nfontFamily:\"Segoe UI\",\ncursor: isLoading  ? \"not-allowed\" : \"pointer\",\nfontWeight: \"400\",\ncursor:\"pointer\"\n}\n\n\t\t\n  return (\n    <div style={wrap} className=\"App\">\n     <label style={labelInput}> Select file <input disabled={isLoading} style={mystyle} type=\"file\" name=\"file\" ref={uploadRef} onChange={UploadFile} />\n\t\t </label>\n      <label className=\"progressLabel\">\n         <progress ref={progressRef} value=\"0\" max=\"100\" />\n      </label>\n    </div>\n  );\n  }\n      \n    const ConnectedComponent = Tooljet.connectComponent(MyCustomComponent);\n    ReactDOM.render(<ConnectedComponent />, document.body);",
